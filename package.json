{
  "name": "maslow",
  "version": "1.0.0",
  "description": "A redux generic module creator for api stuff",
  "main": "dist/bundle.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest --config config.jest.json --coverage",
    "test:watch": "jest --config config.jest.json --watch",
    "lint": "eslint --config .eslintrc lib",
    "lint:fix": "eslint --config .eslintrc lib --fix",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timeforyouapp/maslow.git"
  },
  "keywords": [
    "redux",
    "react",
    "react-redux",
    "jest",
    "es6",
    "api"
  ],
  "author": "Guilherme Diego <guidiego.expgames@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timeforyouapp/maslow/issues"
  },
  "homepage": "https://github.com/timeforyouapp/maslow#readme",
  "dependencies": {
    "docusaurus-init": "^1.0.2",
    "fast-clone": "^1.5.3",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-watch": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-jest": "^24.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.16.0",
    "jest": "^24.1.0"
  }
}
